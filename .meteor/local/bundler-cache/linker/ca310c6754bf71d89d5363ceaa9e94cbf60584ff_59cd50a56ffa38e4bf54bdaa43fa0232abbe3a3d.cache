AAR;AAAA;AAAA;AADF,iBADF;AAMI;AAAA;AAAA;AACI;AAAA;AAAA;AAAQ,qCAAS;AAAA,uCAAMF,SAASG,MAAT,EAAN;AAAA;AAAjB;AACI;AAAG,uCAAU;AAAb,0BADJ;AAAA;AAAA;AADJ;AANJ;AAPJ;AADJ,KADF;AA0BD,CA3BD;;AA6BAvB,cAAcwB,SAAd,GAA0B;AACzBF,WAAOD,UAAUI,MAAV,CAAiBC;AADC,CAA1B;AAjCA7D,OAAOoC,aAAP,CAqCeD,aArCf,oE;;;;;;;;;;;;;;;;;;;;;;;;;ACAAnC,OAAOiB,MAAP,CAAc;AAAC,YAAQ;AAAA,SAAIM,MAAJ;AAAA;AAAT,CAAd;AAAoC,IAAIxB,cAAJ;AAAUC,OAAOC,KAAP,CAAaC,QAAQ,OAAR,CAAb,EAA8B;AAAA,sBAASC,CAAT,EAAW;AAACJ,UAAMI,CAAN;AAAQ;AAApB,CAA9B,EAAoD,CAApD;AAAuD,IAAIkC,aAAJ;AAASrC,OAAOC,KAAP,CAAaC,QAAQ,cAAR,CAAb,EAAqC;AAACmC,KAAD,YAAMlC,CAAN,EAAQ;AAACkC,SAAKlC,CAAL;AAAO;AAAhB,CAArC,EAAuD,CAAvD;AAA0D,IAAIoD,iBAAJ;AAAavD,OAAOC,KAAP,CAAaC,QAAQ,sBAAR,CAAb,EAA6C;AAACqD,SAAD,YAAUpD,CAAV,EAAY;AAACoD,aAASpD,CAAT;AAAW;AAAxB,CAA7C,EAAuE,CAAvE;;IAIhKoB,M;;;AACpB,iBAAYe,KAAZ,EAAmB;AAAA;;AAAA,6DAClB,4BAAMA,KAAN,CADkB;;AAElB,QAAKC,KAAL,GAAa;AACZC,UAAO;AADK,GAAb;AAFkB;AAKlB;;kBACDC,Q;oBAASC,C,EAAG;AAAA;;AACXA,KAAEC,cAAF;AAEA,OAAIC,QAAQ,KAAKC,IAAL,CAAUD,KAAV,CAAgBE,KAAhB,CAAsBC,IAAtB,EAAZ;AACA,OAAIC,WAAW,KAAKH,IAAL,CAAUG,QAAV,CAAmBF,KAAnB,CAAyBC,IAAzB,EAAf;;AAEA,OAAIC,SAASc,MAAT,GAAkB,CAAtB,EAAyB;AACxB,WAAO,KAAKX,QAAL,CAAc;AAACX,YAAO;AAAR,KAAd,CAAP;AACA;;AAEDe,YAASQ,UAAT,CAAoB;AAACnB,gBAAD;AAAQI;AAAR,IAApB,EAAuC,UAACE,GAAD,EAAS;AAC/C,QAAIA,GAAJ,EAAS;AACR,YAAKC,QAAL,CAAc;AAACX,aAAOU,IAAIc;AAAZ,MAAd;AACA,KAFD,MAEO;AACN,YAAKb,QAAL,CAAc;AAACX,aAAO;AAAR,MAAd;AACA;AACD,IAND;AAOA;;;;;kBACD1B,M;oBAAS;AACR,UACC;AAAA;AAAA;AAAK,gBAAU;AAAf;AACC;AAAA;AAAA;AAAK,iBAAU;AAAf;AACC;AAAA;AAAA;AAAA;AAAA,MADD;AAGE,UAAKyB,KAAL,CAAWC,KAAX,GAAmB;AAAA;AAAA;AAAI,WAAKD,KAAL,CAAWC;AAAf,MAAnB,GAA+CY,SAHjD;AAKC;AAAA;AAAA;AAAM,iBAAU,KAAKX,QAAL,CAAcY,IAAd,CAAmB,IAAnB,CAAhB;AAA0C,uBAA1C;AAAqD,kBAAU;AAA/D;AACC;AAAO,aAAK,OAAZ;AAAoB,YAAI,OAAxB;AAAgC,aAAK,OAArC;AAA6C,oBAAY;AAAzD,QADD;AAEC;AAAO,aAAK,UAAZ;AAAuB,YAAI,UAA3B;AAAsC,aAAK,UAA3C;AAAsD,oBAAY;AAAlE,QAFD;AAGC;AAAA;AAAA;AAAQ,mBAAU;AAAlB;AAAA;AAAA;AAHD,MALD;AAWC;AAAC,UAAD;AAAA;AAAM,WAAG;AAAT;AAAA;AAAA;AAXD;AADD,IADD;AAkBA;;;;;;EA5CkCtD,MAAMuD,S","file":"/app.js","sourcesContent":["import React from 'react';\nimport ReactDOM from 'react-dom';\nimport { Tracker } from 'meteor/tracker';\nimport { Session } from 'meteor/session';\n\nimport { routes, onAuthChange } from '../imports/routes/routes';\n\nTracker.autorun(() => {\n  const isAuthenticated = !!Meteor.userId();\n  onAuthChange(isAuthenticated);\n});\n\nMeteor.startup(() => {\n  ReactDOM.render(routes, document.getElementById('app'));\n});\n","import { Meteor } from 'meteor/meteor';\nimport React from 'react';\nimport { Router, Route, browserHistory } from 'react-router';\n\nimport Dashboard from '../ui/Dashboard';\nimport Login from '../ui/Login';\nimport Signup from '../ui/Signup';\nimport NotFound from '../ui/NotFound';\n\nconst unauthenticatedPages = ['/', '/signup'];\nconst authenticatedPages = ['/dashboard'];\nconst onEnterPublicPage = () => {\n\tif (Meteor.userId()) {\n\t\tbrowserHistory.replace('/dashboard');\n\t}\n};\nconst onEnterPrivatePage = () => {\n\tif (!Meteor.userId()) {\n\t\tbrowserHistory.replace('/');\n\t}\n};\nexport const onAuthChange = (isAuthenticated) => {\n\tconst pathname = browserHistory.getCurrentLocation().pathname;\n\tconst isUnauthenticatedPage = unauthenticatedPages.includes(pathname);\n\tconst isAuthenticatedPage = authenticatedPages.includes(pathname);\n\tif (isUnauthenticatedPage && isAuthenticated) {\n\t\tbrowserHistory.replace('/dashboard');\n\t} else if (isAuthenticatedPage && !isAuthenticated) {\n\t\tbrowserHistory.replace('/');\n\t}\n};\nexport const routes = (\n  <Router history={browserHistory}>\n    <Route path=\"/\" component={Login} onEnter={onEnterPublicPage}/>\n    <Route path=\"/signup\" component={Signup} onEnter={onEnterPublicPage}/>\n    <Route path=\"/dashboard\" component={Dashboard} onEnter={onEnterPrivatePage}/>\n    <Route path=\"*\" component={NotFound}/>\n  </Router>\n);\n","import React from 'react';\n\nimport PrivateHeader from './PrivateHeader';\n\nexport default () => {\n\treturn (\n\t\t<div>\n\t\t\t<PrivateHeader title=\"Dashboard\"/>\n\t\t\t<div className=\"page-content\">\n\t\t\t\tDashboard page content.\n\t\t\t</div>\n\t\t</div>\n\t);\n};\n","import React from 'react';\nimport { Link } from 'react-router';\nimport { Meteor } from 'meteor/meteor';\n\nexport default class Login extends React.Component {\n\tconstructor(props) {\n\t\tsuper(props);\n\t\tthis.state = {\n\t\t\terror: ''\n\t\t};\n\t}\n\tonSubmit(e) {\n\t\te.preventDefault();\n\n\t\tlet email = this.refs.email.value.trim();\n\t\tlet password = this.refs.password.value.trim();\n\n\t\tMeteor.loginWithPassword({email}, password, (err) => {\n\t\t\tif (err) {\n\t\t\t\tthis.setState({error: 'Unable to login. Check email and password.'});\n\t\t\t} else {\n\t\t\t\tthis.setState({error: ''});\n\t\t\t}\n\t\t});\n\t}\n\trender() {\n\t\treturn(\n\t\t\t<div className=\"boxed-view\">\n\t\t\t\t<div className=\"boxed-view__box\">\n\t\t\t\t\t<h1>Login</h1>\n\n\t\t\t\t\t{this.state.error ? <p>{this.state.error}</p> : undefined}\n\n\t\t\t\t\t<form onSubmit={this.onSubmit.bind(this)} noValidate className=\"boxed-view__form\">\n\t\t\t\t\t\t<input type=\"email\" ref=\"email\" name=\"email\" placeholder=\"Email\"/>\n\t\t\t\t\t\t<input type=\"password\" ref=\"password\" name=\"password\" placeholder=\"Password\"/>\n\t\t\t\t\t\t<button className=\"button\">Login</button>\n\t\t\t\t\t</form>\n\n\t\t\t\t\t<Link to=\"/signup\">Need an account?</Link>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t);\n\t}\n}\n","import React from 'react';\nimport { Link } from 'react-router';\n\nexport default () => {\n\treturn (\n\t\t<div className=\"boxed-view\">\n\t\t\t<div className=\"boxed-view__box\">\n\t\t\t\t<h1>Page Not Found</h1>\n\t\t\t\t<p>Hmm, we are unable to find that page.</p>\n\t\t\t\t<Link to=\"/\" className=\"button button--link\">HEAD HOME</Link>\n\t\t\t</div>\n\t\t</div>);\n}\n","import React from 'react';\nimport { Accounts } from 'meteor/accounts-base';\nimport PropTypes from 'prop-types';\n\nconst PrivateHeader = (props) => {\n  return (\n    <div className=\"row border-bottom\">\n        <nav className=\"navbar navbar-static-top\" role=\"navigation\">\n            <div className=\"navbar-header\">\n                <a id=\"navbar-minimalize\" className=\"minimalize-styl-2 btn btn-primary \" href=\"#\"><i className=\"fa fa-bars\"></i> </a>\n            </div>\n            <div className=\"nav navbar-top-links navbar-left\">\n                <a>{props.title} TITLE CSS NEEDED</a>\n            </div>\n            <ul className=\"nav navbar-top-links navbar-right\">\n              <li>\n                <a href=\"#\">\n                  Sign Up\n                </a>\n              </li>\n                <li>\n                    <button onClick={() => Accounts.logout()}>\n                        <i className=\"fa fa-sign-out\"></i> Log out\n                    </button>\n                </li>\n\n            </ul>\n\n        </nav>\n    </div>\n  );\n}\n\nPrivateHeader.propTypes = {\n\ttitle: PropTypes.string.isRequired\n};\n\nexport default PrivateHeader;\n","import React from 'react';\nimport { Link } from 'react-router';\nimport { Accounts } from 'meteor/accounts-base';\n\nexport default class Signup extends React.Component {\n\tconstructor(props) {\n\t\tsuper(props);\n\t\tthis.state = {\n\t\t\terror: ''\n\t\t};\n\t}\n\tonSubmit(e) {\n\t\te.preventDefault();\n\n\t\tlet email = this.refs.email.value.trim();\n\t\tlet password = this.refs.password.value.trim();\n\n\t\tif (password.length < 8) {\n\t\t\treturn this.setState({error: 'Password must be more than 8 characters long'});\n\t\t}\n\n\t\tAccounts.createUser({email, password}, (err) => {\n\t\t\tif (err) {\n\t\t\t\tthis.setState({error: err.reason});\n\t\t\t} else {\n\t\t\t\tthis.setState({error: ''});\n\t\t\t}\n\t\t});\n\t}\n\trender() {\n\t\treturn(\n\t\t\t<div className=\"boxed-view\">\n\t\t\t\t<div className=\"boxed-view__box\">\n\t\t\t\t\t<h1>Join</h1>\n\n\t\t\t\t\t{this.state.error ? <p>{this.state.error}</p> : undefined}\n\n\t\t\t\t\t<form onSubmit={this.onSubmit.bind(this)} noValidate className=\"boxed-view__form\">\n\t\t\t\t\t\t<input type=\"email\" ref=\"email\" name=\"email\" placeholder=\"Email\"/>\n\t\t\t\t\t\t<input type=\"password\" ref=\"password\" name=\"password\" placeholder=\"Password\"/>\n\t\t\t\t\t\t<button className=\"button\">Create Account</button>\n\t\t\t\t\t</form>\n\n\t\t\t\t\t<Link to=\"/\">Have an account?</Link>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t);\n\n\t}\n}\n"]}}]